{
	"name": "snake-game",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && vite build",
		"build:obfuscated": "OBFUSCATE_CLIENT=1 npm run build",
		"lint": "eslint .",
		"preview": "npm run build && vite preview",
		"deploy": "npm run build:obfuscated && wrangler deploy -c wrangler.toml",
		"cf-typegen": "wrangler types",
		"test:e2e": "playwright test --config playwright.config.ts"
	},
	"dependencies": {
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"three": "^0.182.0"
	},
	"devDependencies": {
		"@cloudflare/vite-plugin": "^1.22.1",
		"@eslint/js": "^9.33.0",
		"@playwright/test": "^1.55.0",
		"@types/react": "^19.1.10",
		"@types/react-dom": "^19.1.7",
		"@vitejs/plugin-react": "^5.0.0",
		"eslint": "^9.33.0",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.20",
		"globals": "^16.3.0",
		"javascript-obfuscator": "^5.3.0",
		"typescript": "~5.8.3",
		"typescript-eslint": "^8.39.1",
		"vite": "^7.1.2",
		"wrangler": "^4.61.1"
	}
}
